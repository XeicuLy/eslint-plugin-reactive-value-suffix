{
  "name": "eslint-plugin-reactive-value-suffix",
  "version": "3.0.1",
  "description": "This ESLint plugin enforces adding `.value` when accessing reactive values in the Vue 3 Composition API.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint:ci": "eslint .",
    "format:ci": "prettier . --check",
    "type-check:ci": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "eslint",
    "reactive",
    "vue",
    "nuxt"
  ],
  "author": "XeicuLy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XeicuLy/eslint-plugin-reactive-value-suffix"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint/js": "9.21.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/eslint": "9.6.1",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/estree": "1.0.6",
    "@types/node": "22.15.2",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "@vitest/coverage-v8": "3.0.6",
    "@vitest/ui": "3.0.6",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "npm-check-updates": "17.1.15",
    "prettier": "3.5.2",
    "rimraf": "6.0.1",
    "semantic-release": "24.2.3",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1",
    "vite": "6.1.1",
    "vitest": "3.0.6"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,mts,cts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yaml,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "9.15.6"
  }
}
